<!doctype html> 
<html>  
  <head>  
    <title><%=shopName%></title> 
  </head>  
  <body>  
    <h1> This is the register page for Drinks4U </h1> 
        <p> Please enter your details to register </p>
    <form method="POST" action="/registered"> 
  
      <p>
        First Name:<input id="first" type="text" name="first" placeholder="first name" required/> 
      </p>
  
      <p>
        Last Name:<input id="last" type="text" name="last" placeholder="last name" required/> 
      </p>
  
      <p>
        Email:<input id="email" type="email" name="email" placeholder="email" required/>
      </p>

      <input type="submit" value="OK" /> 
    </form> 
</body>  
</html>